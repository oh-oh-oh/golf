  "main": "server/index.ts",
  "scripts": {
    "build": "run-p build:client build:server",
    "build:client:browser": "rm -rf ./dist/client && TARGET_ENV=client NODE_ENV=production ./scripts/build-client.js",
    "build:client:ssr": "rm -rf ./dist/client && TARGET_ENV=server NODE_ENV=production ./scripts/build-client.js",
    "build:client": "run-p build:client:browser build:client:ssr",
    "build:server": "rm -rf ./dist/server && yarn codegen && tsc --project ./server",
    "codegen": "./codegen/run.sh && yarn prisma generate",
    "dev:server": "yarn nodemon -r tsconfig-paths/register -r dotenv/config ./server/index.ts",
    "dev": "yarn codegen && yarn dev:server",
    "postinstall": "patch-package",
    "seed": "ts-node ./prisma/seed.ts",
    "start": "node -r dotenv/config ./dist/server/index.js",
    "test": "echo \"TEST\""
  },
