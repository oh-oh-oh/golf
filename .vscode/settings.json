{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typecript",
    "typecriptreact",
  ],
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "html.validate.styles": true,
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "cSpell.words": [
    "antd",
    "codegen",
    "fastify",
    "mercurius",
    "ohohoh",
    "pino",
    "typegen",
    "vite"
  ]
}
